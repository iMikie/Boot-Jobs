<section>
 <div class="container">
  <div class="row">
    <div class="col s12">
      <% if @skills == nil %>
		<a href="/user/skills/new"><p>You dont have any skills, add one!</p></a>
		<% else %>
		<% @skills.each do |skill| %>
		<%= skill.title %>
	   <% end %>
	  <% end %>
    </div>
  </div>
 </div>
</section>